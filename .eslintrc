{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-essential",
    "eslint:recommended",
    "@vue/typescript/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "ignorePatterns": ["dist/", "docs/", "**/*.min.js", "*.html", "*.md"],
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn"
  }
}
